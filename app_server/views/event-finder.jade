extends layout

block content
  .container(ng-controller="")
    .card.card-container
      a(ng-href='/welcome')
        img(ng-src='/img/bunny.png', alt='logo', width='90', height='90')
      p#profile-name.profile-name-card Find Event
      .welcome
        a.btn.btn-info(ng-href='/events/filter', role='button') BY CATEGORY
        br
        a.btn.btn-info(ng-href='/events/search', role='button') BY KEYWORDS
        br
        a.btn.btn-info(ng-href='/events', role='button') VIEW ALL
        br
