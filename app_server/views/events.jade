extends layout

block content
  .row
    .card-columns.white-bgd.card-shadow.col-xs-10.col-xs-offset-1.col-sm-8.col-sm-offset-2.col-md-6.col-md-offset-3
      a(href='/welcome')
        img#logo.img-responsive.center-block(src='/img/bunny.png', alt='logo', width='90', height='90')
      p#profile-name.profile-name-card #{title}
      br
      p #{msg}
      .all
        ul.nav
          each event in eventsList
            li
              a(href="#{link}/#{event._id}") #{event.title}
